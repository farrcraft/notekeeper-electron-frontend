{
  "name": "notekeeper-electron",
  "productName": "NoteKeeper.io",
  "description": "NoteKeeper.io desktop application",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": ""
  },
  "version": "0.1.0",
  "scripts": {
    "build": "concurrently \"yarn build-main\" \"yarn build-preload\" \"yarn build-renderer\"",
    "build-dev": "concurrently \"yarn build-main-dev\" \"yarn build-preload-dev\" \"yarn build-renderer-dev\"",
    "build-preload": "cross-env NODE_ENV=production webpack --config ./webpack/prod/webpack.config.preload.prod.babel.js --color",
    "build-main": "cross-env NODE_ENV=production webpack --config ./webpack/prod/webpack.config.main.prod.babel.js --color",
    "build-renderer": "cross-env NODE_ENV=production webpack --config ./webpack/prod/webpack.config.renderer.prod.babel.js --color",
    "start": "cross-env NODE_ENV=production electron ./app/dist/prod/main.prod.js",
    "build-renderer-dev": "cross-env NODE_ENV=development node -r @babel/register ./node_modules/webpack/bin/webpack --config ./webpack/dev/webpack.config.renderer.dev.babel.js --color",
    "build-main-dev": "cross-env NODE_ENV=development node -r @babel/register ./node_modules/webpack/bin/webpack --config ./webpack/dev/webpack.config.main.dev.babel.js --color",
    "build-preload-dev": "cross-env NODE_ENV=development node -r @babel/register ./node_modules/webpack/bin/webpack --config ./webpack/dev/webpack.config.preload.dev.babel.js --color",
    "start-dev": "cross-env NODE_ENV=development electron ./app/dist/dev/main.dev.js",
    "run-dev": "yarn build-dev && yarn start-dev",
    "type-check": "tsc --noEmit --pretty",
    "build-types": "tsc --emitDeclarationOnly",
    "lint": "cross-env NODE_ENV=development eslint --cache --format=pretty --ext .ts,.tsx .",
    "lint-fix": "yarn --silent lint --fix; exit 0",
    "package": "yarn build && electron-builder build --publish never",
    "package-all": "yarn build && electron-builder build -mwl",
    "package-ci": "yarn postinstall && yarn build && electron-builder --publish always",
    "package-linux": "yarn build && electron-builder build --linux",
    "package-win": "yarn build && electron-builder build --win --x64",
    "postinstall": "electron-builder install-app-deps package.json"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "cross-env NODE_ENV=development eslint --cache --format=pretty",
      "git add"
    ]
  },
  "main": "./app/dist/main.prod.js",
  "build": {
    "productName": "NoteKeeper.io",
    "appId": "org.NoteKeeper",
    "category": "public.app-category.tools",
    "files": [
      "app/dist/",
      "app/app.html",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@babel/eslint-parser": "7.17.0",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-decorators": "7.17.9",
    "@babel/plugin-proposal-do-expressions": "7.16.7",
    "@babel/plugin-proposal-export-default-from": "7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "7.16.7",
    "@babel/plugin-proposal-function-bind": "7.16.7",
    "@babel/plugin-proposal-function-sent": "7.16.7",
    "@babel/plugin-proposal-json-strings": "7.16.7",
    "@babel/plugin-proposal-logical-assignment-operators": "7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.16.7",
    "@babel/plugin-proposal-numeric-separator": "7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "7.17.3",
    "@babel/plugin-proposal-optional-chaining": "7.16.7",
    "@babel/plugin-proposal-pipeline-operator": "7.17.6",
    "@babel/plugin-proposal-throw-expressions": "7.16.7",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-meta": "7.10.4",
    "@babel/plugin-transform-react-constant-elements": "7.17.6",
    "@babel/plugin-transform-react-inline-elements": "7.16.7",
    "@babel/preset-env": "7.17.10",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@babel/register": "7.17.7",
    "@types/base64-js": "1.3.0",
    "@types/electron-devtools-installer": "2.2.2",
    "@types/google-protobuf": "3.15.6",
    "@types/node": "17.0.31",
    "@types/source-map-support": "0.5.4",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "babel-loader": "8.2.5",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "classnames": "2.3.1",
    "concurrently": "7.1.0",
    "cross-env": "7.0.3",
    "electron": "18.2.0",
    "electron-builder": "23.0.3",
    "electron-devtools-installer": "3.2.0",
    "electron-rebuild": "3.2.7",
    "eslint": "8.15.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-formatter-pretty": "4.1.0",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "8.0.1",
    "lint-staged": "12.4.1",
    "terser-webpack-plugin": "5.3.1",
    "ts-protoc-gen": "0.15.0",
    "typescript": "4.6.4",
    "url-loader": "4.1.1",
    "webpack": "5.72.1",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.8.0",
    "yarn": "1.22.18"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.1.1",
    "@mui/material": "5.7.0",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "base64-js": "1.5.1",
    "core-js": "3.22.5",
    "devtron": "1.4.0",
    "electron-log": "4.4.7",
    "electron-updater": "5.0.1",
    "google-protobuf": "3.20.1",
    "history": "5.3.0",
    "mobx": "6.5.0",
    "mobx-react": "7.4.0",
    "moment": "2.29.3",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-modal": "3.15.1",
    "source-map-support": "0.5.21",
    "tweetnacl": "1.0.3",
    "winston": "3.7.2"
  },
  "engines": {
    "node": ">=16.x <17",
    "yarn": ">=1.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
