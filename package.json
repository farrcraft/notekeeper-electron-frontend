{
  "name": "notekeeper-electron",
  "productName": "NoteKeeper.io",
  "description": "NoteKeeper.io desktop application",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": ""
  },
  "version": "0.1.0",
  "scripts": {
    "build": "concurrently \"yarn build-main\" \"yarn build-preload\" \"yarn build-renderer\"",
    "build-dev": "concurrently \"yarn build-main-dev\" \"yarn build-preload-dev\" \"yarn build-renderer-dev\"",
    "build-preload": "cross-env NODE_ENV=production webpack --config ./webpack/prod/webpack.config.preload.prod.babel.js --colors",
    "build-main": "cross-env NODE_ENV=production webpack --config ./webpack/prod/webpack.config.main.prod.babel.js --colors",
    "build-renderer": "cross-env NODE_ENV=production webpack --config ./webpack/prod/webpack.config.renderer.prod.babel.js --colors",
    "start": "cross-env NODE_ENV=production electron ./app/dist/prod/main.prod.js",
    "build-renderer-dev": "cross-env NODE_ENV=development node -r @babel/register ./node_modules/webpack/bin/webpack --config ./webpack/dev/webpack.config.renderer.dev.babel.js --colors",
    "build-main-dev": "cross-env NODE_ENV=development node -r @babel/register ./node_modules/webpack/bin/webpack --config ./webpack/dev/webpack.config.main.dev.babel.js --colors",
    "build-preload-dev": "cross-env NODE_ENV=development node -r @babel/register ./node_modules/webpack/bin/webpack --config ./webpack/dev/webpack.config.preload.dev.babel.js --colors",
    "start-dev": "cross-env NODE_ENV=development electron ./app/dist/dev/main.dev.js",
    "run-dev": "yarn build-dev && yarn start-dev",
    "type-check": "tsc --noEmit --pretty",
    "build-types": "tsc --emitDeclarationOnly",
    "lint": "cross-env NODE_ENV=development eslint --cache --format=pretty --ext .ts,.tsx .",
    "lint-fix": "yarn --silent lint --fix; exit 0",
    "package": "yarn build && electron-builder build --publish never",
    "package-all": "yarn build && electron-builder build -mwl",
    "package-ci": "yarn postinstall && yarn build && electron-builder --publish always",
    "package-linux": "yarn build && electron-builder build --linux",
    "package-win": "yarn build && electron-builder build --win --x64",
    "postinstall": "electron-builder install-app-deps package.json"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "cross-env NODE_ENV=development eslint --cache --format=pretty",
      "git add"
    ]
  },
  "main": "./app/dist/main.prod.js",
  "build": {
    "productName": "NoteKeeper.io",
    "appId": "org.NoteKeeper",
    "category": "public.app-category.tools",
    "files": [
      "app/dist/",
      "app/app.html",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.6.0",
    "@babel/plugin-proposal-do-expressions": "7.6.0",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "7.5.2",
    "@babel/plugin-proposal-function-bind": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.5.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.4.4",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-proposal-optional-chaining": "7.6.0",
    "@babel/plugin-proposal-pipeline-operator": "7.5.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-react-constant-elements": "7.6.3",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@babel/preset-typescript": "7.6.0",
    "@babel/register": "7.6.2",
    "@types/base64-js": "1.2.5",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/google-protobuf": "3.7.2",
    "@types/node": "12.11.1",
    "@types/source-map-support": "0.5.0",
    "@typescript-eslint/eslint-plugin": "2.4.0",
    "@typescript-eslint/parser": "2.4.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "classnames": "2.2.6",
    "concurrently": "5.0.0",
    "cross-env": "6.0.3",
    "electron": "6.0.12",
    "electron-builder": "21.2.0",
    "electron-devtools-installer": "2.2.4",
    "electron-rebuild": "1.8.6",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-formatter-pretty": "2.1.1",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-compat": "3.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.1.2",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "terser-webpack-plugin": "2.1.3",
    "ts-protoc-gen": "0.11.0",
    "typescript": "3.6.4",
    "url-loader": "2.2.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.9",
    "webpack-merge": "4.2.2",
    "yarn": "1.19.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.11.2",
    "@material-ui/core": "4.5.1",
    "@material-ui/icons": "4.5.1",
    "@material-ui/styles": "4.5.0",
    "base64-js": "1.3.1",
    "core-js": "3.3.2",
    "devtron": "1.4.0",
    "electron-log": "3.0.8",
    "electron-updater": "4.1.2",
    "google-protobuf": "3.10.0",
    "history": "4.10.1",
    "mobx": "5.14.2",
    "mobx-react": "6.1.4",
    "moment": "2.24.0",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-modal": "3.10.1",
    "source-map-support": "0.5.13",
    "tweetnacl": "1.0.1",
    "winston": "3.2.1"
  },
  "engines": {
    "node": ">=12.x <14",
    "yarn": ">=1.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
